<%= simple_form_for(@card_processing, html: { multipart: true }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :exam_date, as: :date %>
    <%= f.input :is_bolsao %>
    <%= f.input :file, as: :file %>
    <%= f.association :card_type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
