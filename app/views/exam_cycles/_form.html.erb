<%= simple_form_for(@exam_cycle) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :is_bolsao %>
    <%= f.association :product_year %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
