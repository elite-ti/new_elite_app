<%= title 'Edit Exam Day', link_to('Back', exam_days_path) %>

<%= simple_form_for(@exam_day) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :datetime %>
    <%= f.association :super_exam %>
    <%= f.association :exam_cycle %>
    <%= f.association :super_klazz %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
