<%= simple_form_for(@answer_card_type) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :parameters %>
    <%= f.input :card %>
    <%= f.input :student_number_length %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
