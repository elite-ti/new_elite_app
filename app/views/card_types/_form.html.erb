<%= simple_form_for(@card_type) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :parameters %>
    <%= f.input :card %>
    <%= f.input :student_coordinates %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
